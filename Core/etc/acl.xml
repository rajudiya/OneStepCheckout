<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Seofeb_Core::menu" title="Seofeb Extensions" sortOrder="50" >
                    <resource id="Seofeb_Core::documentation" title="Documentations" sortOrder="50" >
                        <resource id="Seofeb_Core::userguide" title="User Guides" sortOrder="50" />
                    </resource>
                    <resource id="Seofeb_Core::activate" title="Active Module" sortOrder="70" />
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Seofeb_Core::configuration" title="Seofeb Information" sortOrder="10"/>
                            <resource id="Seofeb_Core::marketplace" title="Seofeb Marketplace" sortOrder="10"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
